@baseURL=http://localhost:8000

@urlPath=api/v1/reaction


### Get reaction endpoint for a particular post
GET {{baseURL}}/{{urlPath}}/6454a42b926e57a53834ad0c
Content-Type: application/json
Accept: application/json
withCredentials:true

### Get single reaction for a particular user
GET {{baseURL}}/{{urlPath}}/single/Marian/6454a42b926e57a53834ad0c
Content-Type: application/json
Accept: application/json
withCredentials:true

### Get all the reactions of an user
GET {{baseURL}}/{{urlPath}}/all/Marian
Content-Type: application/json
Accept: application/json
withCredentials:true


### Add a reaction endpoint
POST {{baseURL}}/{{urlPath}}/add-reaction
Content-Type: application/json
Accept: application/json
withCredentials:true

{
 "userTo":"643bfb226b9a22fdc00550df",
 "postId":"6454a42b926e57a53834ad0c",
 "type":"like",
 "profilePicture":"data:image/png;base64,",
 "previousReaction":"",
 "postReactions": {"like": 1,"love": 0,"happy": 0,"sad": 0,"wow": 0,"angry": 0}
}


### Remove reaction endpoint
DELETE {{baseURL}}/{{urlPath}}/6454a42b926e57a53834ad0c/like/{"like": 0,"love": 0,"happy": 0,"sad": 0,"wow": 0,"angry": 0}

Content-Type: application/json
Accept: application/json
withCredentials:true


